clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod4

keycode 9 = Escape
keycode 66 = Caps_Lock

keycode 22 = Control_L
keycode 37 = Super_L
keycode 134 = BackSpace
keycode 119 = Alt_L
keycode 64 = Delete
keycode 105 = Mode_switch

add Lock = Caps_Lock
add Control = Control_L Control_R
add Mod1 = Alt_L Alt_R
add Mod4 = Super_L Super_R
add Mod5 = Mode_switch

keycode  35 = slash question slash question
keycode  34 = equal plus equal plus
