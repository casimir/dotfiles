clear Control
clear Mod1
clear Mod2
clear Mod4

clear Lock
keysym Escape = Escape
keysym Caps_Lock = Caps_Lock
add Lock = Caps_Lock

keycode 37 = Super_L
keycode 134 = Control_L
keycode 64 = Mode_switch
keycode 105 = Alt_R

add Control = Control_L
add Mod1 = Alt_L Alt_R
add Mod4 = Super_L
add Mod5 = Mode_switch

keycode  35 = slash question slash question
keycode  34 = equal plus equal plus
