clear Lock
clear Control
clear Mod1
clear Mod4
clear Mod5

keycode 9 = Caps_Lock
keycode 66 = Escape

keycode 37 = Super_L
keycode 64 = Control_L
keycode 119 = Alt_L
keycode 134 = Delete
keycode 105 = Mode_switch

add Lock = Caps_Lock
add Control = Control_L
add Mod4 = Super_L
add Mod1 = Alt_L
add Mod5 = Mode_switch

keycode  35 = slash question slash question
keycode  34 = equal plus equal plus
